{
    "id": "scene-media-limits",
    "$schema": "http://json-schema.org/draft-04/schema#",
    "title": "SceneMediaLimitsConfig",
    "description": "The limits of a panoramic media object. An instance of <a href=\"#scene-media-limits\">a scene media limits object.</a>",
    "type": "object",

    "properties": {

        "yaw": {
            "type": "SceneMediaRangeConfig",
            "title": "Yaw range",
            "description": "Horizontal angular range value."
        },

        "pitch": {
            "type": "SceneMediaRangeConfig",
            "title": "Pitch range",
            "description": "Vertical angular range value."
        }
    }
}
